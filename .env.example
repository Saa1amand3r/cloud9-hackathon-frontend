# Cloudy Poro Frontend Configuration
# Copy this file to .env and update the values

# Backend API URL (default: http://localhost:8000)
VITE_API_BASE_URL=http://localhost:8000

# WebSocket URL for report generation (default: ws://localhost:8000/ws/report)
VITE_WS_URL=ws://localhost:8000/ws/report

# Use mock data instead of real backend (default: false)
# Set to "true" for development without backend
VITE_USE_MOCK=false

# Your team name for matchup context (default: Cloud9)
VITE_OUR_TEAM=Cloud9
